pipeline{
    agent any
         stages{
             stage(git clone){
                 steps{
                     git 'https://github.com/Akash25dev/Git-repo.git'
                 }
             }
             stage(build with mvn){
                 steps{
                     sh 'mvn install'
                 }
             }
             stage(deploy){
                 steps{  
                   sh 'cp target/Git-repo.war /home/akash/Documents/Maven/apache-tomcat-9.0.93/webapps'
                 }
              }
         }
} 
                  
